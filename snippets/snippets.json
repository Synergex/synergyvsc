{
    "OPEN_CHANNEL": {
        "prefix": "open",
        "body": [
            "open(${1:number}, ${2:record},${3:label})"
        ],
        "description": "Open Channel"
    },
    "USING_LOOP": {
        "prefix": "using",
        "body": [
            "\tusing $1 select",
            "\t\t($2), begin",
            "\t\tend",
            "\tendusing\r"
        ],
        "description": "Using Loop"
    }
}